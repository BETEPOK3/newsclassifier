{
  "logger": {
    "path": "/var/logs",
    "filename": "access.log",
    "level": "info",
    "rotation": "20 days",
    "retention": "1 months",
    "format": " [TEAM2-SYSTEM] - web - <level>{level: <8}</level> <green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> - <cyan>{name}</cyan>:<cyan>{function}</cyan> - <level>{message}</level>"

  }
}